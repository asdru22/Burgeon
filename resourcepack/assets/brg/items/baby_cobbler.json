{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data",
    "cases": [
      {
        "when": "age0",
        "model": {
          "type": "minecraft:model",
          "model": "brg:block/baby_cobbler"
        }
      },
      {
        "when": "age1",
        "model": {
          "type": "minecraft:composite",
          "models": [
            {
              "type": "minecraft:model",
              "model": "brg:block/cobbler/body"
            },
            {
              "type": "minecraft:model",
              "model": "brg:block/cobbler/head_open"
            }
          ]
        }
      },
      {
        "when": "closed",
        "model": {
          "type": "minecraft:composite",
          "models": [
            {
              "type": "minecraft:model",
              "model": "brg:block/cobbler/body"
            },
            {
              "type": "minecraft:model",
              "model": "brg:block/cobbler/head"
            }
          ]
        }
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "brg:item/baby_cobbler"
    }
  }
}
